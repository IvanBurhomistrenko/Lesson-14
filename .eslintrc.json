{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-plusplus":"off",
        "linebreak-style":"off",
        "no-console":"off",
        "arrow-parens":"off",
        "no-shadow":"off",
        "no-alert":"off",
        "consistent-return":"off",
        "prefer-const":"off",
        "no-undef":"off",
        "no-param-reassign":"off"
    }
}
